{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {}
	},
	"plugins": ["prettier"],
	"rules": {
		// "prettier/prettier": "error"
		"no-unused-vars": "warn",
		"no-empty": "off",
		"no-undef": "warn"
	}
}
